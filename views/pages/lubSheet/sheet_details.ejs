                                <%for(var i=1; i<10;i++){%>
                                  <%if(maintFreq.type == i){%>
                                    <td class="col">
                                    <%if(maintFreq.change){%>
                                      <h5>C</h5>
                                    <%}%>
                                    </td>
                                  <%}%>
                                <%}%>
                              <%}%>
                            <%}%>
                          </tr>
                        <%}%>
                      <%}%>
                    <%}%>
                  <%}%>
                </tbody>
              </table>
            </div>
          </div>
        <%}%>

      </div>
    </div>
  </div>
</div>
