          <%console.log(r);%>
          <%if(r==0){%>
            <%spanishName='Líquido Refrigerante';name = 'coolant';%>
          <%}else if(r==1){%>
            <%spanishName='Líquido de Frenos';name = 'break';%>
          <%}else if(r==2){%>
            <%spanishName='Otros Líquidos';name = 'other';%>
          <%}%>
          <div class="row"><!--Liquid-->
